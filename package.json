{
  "name": "perf-service",
  "version": "1.0.0",
  "description": "performance library gateway to store ttfb, fcp, dom load and window load events as well as network timings for document, image, font, js and css that is being collected using perf-lib",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts --ignore-path .gitignore --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://asimyildiz@github.com/asimyildiz/perf-service.git"
  },
  "keywords": [
    "performance",
    "library",
    "gateway",
    "service"
  ],
  "author": "ASIM YILDIZ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asimyildiz/perf-service/issues"
  },
  "homepage": "https://github.com/asimyildiz/perf-service#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mongoose": "^5.12.14"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.23",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "mongodb-memory-server": "^6.9.6",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4"
  }
}
